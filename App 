            <p class="text-lg text-gray-700 dark:text-gray-300">Securely make payments and transfers. A simple way to manage your funds.</p>
            <div class="mt-6">
                <img src="https://placehold.co/400x200/a5d6a7/4caf50?text=Payment+Options" alt="Payment Placeholder" class="rounded-xl mx-auto shadow-md">
            </div>
        </div>

        <!-- Custom Dialog Box -->
        <div id="dialog-box" class="hidden fixed inset-0 bg-gray-900 bg-opacity-75 flex items-center justify-center p-4">
            <div class="bg-white dark:bg-gray-800 p-6 rounded-xl shadow-2xl max-w-sm w-full space-y-4">
                <div class="flex justify-between items-center">
                    <h3 id="dialog-title" class="text-xl font-bold text-gray-900 dark:text-gray-50">Alert</h3>
                    <button id="dialog-close-btn" class="text-gray-400 hover:text-gray-600 dark:hover:text-gray-300">&times;</button>
                </div>
                <p id="dialog-message" class="text-gray-600 dark:text-gray-300"></p>
                <button id="dialog-ok-btn" class="w-full p-3 rounded-xl bg-blue-500 text-white font-bold hover:bg-blue-600 transition">OK</button>
            </div>
        </div>
    </main>

    <script>
        const descriptionInput = document.getElementById('description-input');
        const amountInput = document.getElementById('amount-input');
        const incomeBtn = document.getElementById('income-btn');
        const expenseBtn = document.getElementById('expense-btn');
        const transactionsList = document.getElementById('transactions-list');
        const balanceAmount = document.getElementById('balance-amount');
        const incomeAmount = document.getElementById('income-amount');
        const expenseAmount = document.getElementById('expense-amount');

        const sideMenu = document.getElementById('side-menu');
        const openMenuBtn = document.getElementById('open-menu-btn');
        const closeMenuBtn = document.getElementById('close-menu-btn');
        const menuButtons = document.querySelectorAll('.menu-btn');
        const pages = document.querySelectorAll('.page');
        const learnMoreButtons = document.querySelectorAll('.learn-more-btn');

        const dialogBox = document.getElementById('dialog-box');
        const dialogTitle = document.getElementById('dialog-title');
        const dialogMessage = document.getElementById('dialog-message');
        const dialogCloseBtn = document.getElementById('dialog-close-btn');
        const dialogOkBtn = document.getElementById('dialog-ok-btn');

        let transactions = [];

        // Dialog Content for "Learn More" buttons
        const dialogContent = {
            conservative: {
                title: "Conservative Portfolio",
                message: "A conservative portfolio focuses on capital preservation and generating modest, consistent returns. It is ideal for investors with a low tolerance for risk and a long-term financial horizon. This strategy typically includes a high percentage of low-risk assets like government bonds and cash equivalents."
            },
            balanced: {
                title: "Balanced Portfolio",
                message: "A balanced portfolio aims for a mix of capital growth and income. It's designed for investors with a moderate risk tolerance, balancing the stability of fixed-income securities with the growth potential of stocks. It represents a middle ground between conservative and aggressive strategies."
            },
            aggressive: {
                title: "Aggressive Portfolio",
                message: "An aggressive portfolio seeks to maximize returns by taking on higher risk. It is suited for investors with a high risk tolerance and a long investment horizon, who are comfortable with significant market fluctuations. This portfolio is heavily weighted toward high-growth assets such as stocks and emerging market investments."
            },
            alert: {
                title: "Alert",
                message: "Please enter a valid description and a positive amount."
            }
        };

        // --- Menu and Page Switching Logic ---
        function openMenu() {
            sideMenu.classList.remove('-translate-x-full');
            sideMenu.classList.add('translate-x-0');
        }

        function closeMenu() {
            sideMenu.classList.remove('translate-x-0');
            sideMenu.classList.add('-translate-x-full');
        }

        function showPage(pageId) {
            pages.forEach(page => {
                page.classList.add('hidden');
            });
            document.getElementById(pageId).classList.remove('hidden');

            menuButtons.forEach(btn => {
                btn.classList.remove('active-menu-link');
                btn.classList.remove('bg-gray-100', 'dark:bg-gray-700');
            });

            const activeButton = document.getElementById(pageId + '-btn');
            if (activeButton) {
                activeButton.classList.add('active-menu-link');
                activeButton.classList.add('bg-gray-100', 'dark:bg-gray-700');
            }
            closeMenu();
        }

        // --- Dialog Functions ---
        function showDialog(type) {
            const content = dialogContent[type];
            dialogTitle.textContent = content.title;
            dialogMessage.textContent = content.message;
            dialogBox.classList.remove('hidden');
        }

        function closeDialog() {
            dialogBox.classList.add('hidden');
        }

        // --- Core App Logic (Transactions) ---
        function saveTransactions() {
            localStorage.setItem('transactions', JSON.stringify(transactions));
        }

        function loadTransactions() {
            const savedTransactions = JSON.parse(localStorage.getItem('transactions') || '[]');
            transactions = savedTransactions;
            renderTransactions();
            updateSummary();
        }

        function updateSummary() {
            let totalIncome = 0;
            let totalExpense = 0;
            transactions.forEach(transaction => {
                if (transaction.type === 'income') {
                    totalIncome += transaction.amount;
                } else {
                    totalExpense += transaction.amount;
                }
            });
            const balance = totalIncome - totalExpense;
            balanceAmount.textContent = `$${balance.toFixed(2)}`;
            incomeAmount.textContent = `$${totalIncome.toFixed(2)}`;
            expenseAmount.textContent = `$${totalExpense.toFixed(2)}`;
        }

        function renderTransactions() {
            transactionsList.innerHTML = '';
            if (transactions.length === 0) {
                const emptyMessage = document.createElement('li');
                emptyMessage.className = 'text-center text-gray-500 dark:text-gray-400 p-4';
                emptyMessage.textContent = 'No transactions yet.';
                transactionsList.appendChild(emptyMessage);
                return;
            }
            transactions.forEach((transaction, index) => {
                const li = document.createElement('li');
                li.className = `flex items-center justify-between p-4 rounded-xl shadow-sm ${
                    transaction.type === 'income' ? 'bg-green-50 dark:bg-green-900/50 border-l-4 border-green-500' : 'bg-red-50 dark:bg-red-900/50 border-l-4 border-red-500'
                }`;
                const descriptionDiv = document.createElement('div');
                descriptionDiv.className = 'flex-1';
                descriptionDiv.innerHTML = `
                    <p class="font-semibold">${transaction.description}</p>
                    <p class="text-sm text-gray-500 dark:text-gray-400">${new Date(transaction.timestamp).toLocaleDateString()}</p>
                `;
                const amountDiv = document.createElement('div');
                amountDiv.className = `font-bold ${
                    transaction.type === 'income' ? 'text-green-600 dark:text-green-300' : 'text-red-600 dark:text-red-300'
                }`;
                amountDiv.textContent = `${transaction.type === 'income' ? '+' : '-'}$${transaction.amount.toFixed(2)}`;
                li.appendChild(descriptionDiv);
                li.appendChild(amountDiv);
                transactionsList.appendChild(li);
            });
        }

        function addTransaction(type) {
            const description = descriptionInput.value.trim();
            const amount = parseFloat(amountInput.value);

            if (description === '' || isNaN(amount) || amount <= 0) {
                showDialog('alert');
                return;
            }

            const newTransaction = {
                description,
                amount,
                type,
                timestamp: new Date().toISOString()
            };

            transactions.unshift(newTransaction);
            saveTransactions();
            renderTransactions();
            updateSummary();

            descriptionInput.value = '';
            amountInput.value = '';
        }

        // --- Event Listeners ---
        incomeBtn.addEventListener('click', () => addTransaction('income'));
        expenseBtn.addEventListener('click', () => addTransaction('expense'));
        openMenuBtn.addEventListener('click', openMenu);
        closeMenuBtn.addEventListener('click', closeMenu);
        dialogCloseBtn.addEventListener('click', closeDialog);
        dialogOkBtn.addEventListener('click', closeDialog);
        window.addEventListener('load', loadTransactions);

        // Event listeners for page navigation
        document.getElementById('transactions-page-btn').addEventListener('click', () => showPage('transactions-page'));
        document.getElementById('save-page-btn').addEventListener('click', () => showPage('save-page'));
        document.getElementById('buy-data-page-btn').addEventListener('click', () => showPage('buy-data-page'));
        document.getElementById('invest-page-btn').addEventListener('click', () => showPage('invest-page'));
        document.getElementById('service-page-btn').addEventListener('click', () => showPage('service-page'));
        document.getElementById('contact-page-btn').addEventListener('click', () => showPage('contact-page'));
        document.getElementById('payment-page-btn').addEventListener('click', () => showPage('payment-page'));

        // Event listeners for Learn More buttons
        learnMoreButtons.forEach(button => {
            button.addEventListener('click', (e) => {
                const type = e.target.dataset.type;
                showDialog(type);
            });
        });

    </script>
</body>
</html>
